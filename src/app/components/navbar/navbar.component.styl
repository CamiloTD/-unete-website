    width 5rem

.center
    width: 100%
    display: grid
    align-items: center
    align-content: center
    justify-items: center
.navbar
    width: 100%
    display grid
    grid-template-columns: 9fr 1fr 2fr 2fr 3fr
    font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu, Cantarell, 'Open Sans', 'Helvetica Neue', sans-serif

.unete
    display: grid
    grid-template-columns: 1fr 7fr
    //justify-items: center
    align-items: center
    color: #111111
    text-decoration-line: none
    font-size: 2.1rem
    font-weight: 500

.blue
    color: #4353FF
    font-weight: normal
    text-decoration-line: none

.clearblue  
    display: grid
    background-color: #ECEEFF
    width 12rem
    height: 3rem
    align-items: center
    justify-items: center

.right
